<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="pub.gll.onepeas.module.car" >

    <uses-sdk
        android:minSdkVersion="24"
        android:targetSdkVersion="33" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application>
        <activity
            android:name="pub.gll.onepeas.module.car.activity.CarActivity"
            android:theme="@style/Theme.AppCompat" />
        <activity
            android:name="pub.gll.onepeas.module.car.activity.CarComposeActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:exported="true"
            android:hardwareAccelerated="true"
            android:screenOrientation="fullSensor"
            android:theme="@style/Theme.OnePeasToDo" >
        </activity>
        <activity
            android:name="pub.gll.onepeas.module.car.activity.CustomViewAct"
            android:theme="@style/Theme.AppCompat" />

        <provider
            android:name="androidx.startup.InitializationProvider"
            android:authorities="dollar_openBracket_applicationId_closeBracket.androidx.startup"
            android:exported="false"
            tools:node="merge"
            tools:replace="android:authorities" >
            <meta-data
                android:name="pub.gll.onepeas.module.car.init.AppInitializer"
                android:value="androidx.startup" />
        </provider>
    </application>

</manifest>